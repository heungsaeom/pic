void main()
{

   _CaiDatLCD();
   
   lcd_gotoxy(1,1);
   printf(lcd_putc,"RFID RC522");
   
   while(TRUE)
   {
      //TODO: User Code
   }

}
//viet noi dung cho cac chuong trinh con da duoc dinh nghia truoc do
//*****************************************************************************
//*****************************************************************************
void _CaiDatLCD(void)
{
   lcd_init();//khoi dong module LCD
   lcd_putc('\f');//xoa trang man hinh LCD
}

